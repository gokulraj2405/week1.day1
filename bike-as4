package week1.day1;

public class Bike {
	public void newBike() {
		System.out.println("this is new bike");

	}

	public static void main(String[] args) {
		System.out.println("calling the object form car class");
		Car c1=new Car();
		c1.applybreak();
		c1.soundHorn();
		System.out.println("this method calling from bike");
		Bike b1=new Bike();
		b1.newBike();
		

	}

}
